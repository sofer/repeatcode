<div id="header">
	<h1 class="top-left pad">
		<%= link_to 'Courses', courses_path %>
		&raquo; <%= link_to 'Materials', subjects_path %>
		&raquo; New
	</h1>
</div>
<div id="main">
	<% form_for(@subject) do |f| %>
		<%= render :partial => 'form', :locals => {:f => f } %>
	  <br><br>
		<%= f.submit "Create" %>
	<% end %>
</div>

<div id="footer">
	<%= render :partial => 'global/toplevel_links' %>
</div>